int main()
{
	int T;
	cin>>T;
	while(T--)
	{
		cin>>n;
		for(int i = 0; i < n+1; i++)
		{
			cin>>maze[i];
			for(int j = 0; j < n+1; j++)
				need[i][j] = isdigit(maze[i][j]) ? maze[i][j]-'0' : 0;
		}
		for(int i = 0; i < n*n*2; i++)
			head[i] = -1;
		prepare();
		dfs(0);
		for(int i = 0; i < n; i++)
		{
			for(int j = 0; j < n; j++)
			{
				if (isnul(i*n+j))
					cout<<'0';
				else 
					cout<<(-1 == head[i*n+j] ? '\\' : '/');
			}
			cout<<endl;
		}
	}
}
