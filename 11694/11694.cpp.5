#include <iostream>
#include <vector>
using namespace std;
const int maxn = 7;

int n;
char maze[maxn+1][maxn+1];
int need[maxn+1][maxn+1];

int head[maxn*maxn*2];		// /: i*n+j  \:i*n+j+n*n

int dx[4] = {-1, -1, 0,  0};
int dy[4] = {-1,  0, 0, -1};
char dc[4] = {'\\', '/', '\\', '/'};
char ndc[4] = {'/', '\\', '/', '\\'};

bool isnul(int id)
{
	return -1 == head[id] && -1 == head[(id < n*n ? id+n*n : id-n*n)];
}

bool isnul(int x, int y, int d)
{
	int nx = x+dx[d], ny = y+dy[d];
	if (nx >= 0 && nx < n && ny >= 0 && ny < n)
		return isnul(nx*n+ny);
	return false;
}

bool iscon(int x, int y, int d)
{
	int nx = x+dx[d], ny = y+dy[d];
	if (nx >= 0 && nx < n && ny >= 0 && ny < n)
	{
		int id = nx*n+ny+ ('\\' == dc[d] ? n*n : 0);
		return -1 != head[id];
	}
	return false;
}

int gethead(int id)
{
	if (head[id] < 0)
		return -1;
	int r = id;
	while(r != head[r])
	{
		r = head[r];
	}
	return r;
}

bool isUnit(int id, int & h0, int & h1)
{
	char ch = id < n*n ? '/' : '\\';
	id -= (id < n*n ? 0 : n*n);
	int x = id/n, y = id%n;
	int gx0 = x,   gy0 = ('/' == ch ? y+1 : y);
	int gx1 = x+1, gy1 = ('/' == ch ? y : y+1);
	for(int d = 0; d < 4 && h0 < 0 && h1 < 0; d++)
	{
		if (h0 < 0 && iscon(gx0, gy0, d))
		{
			int p = (gx0+dx[d])*n+(gy0+dy[d]);
			p += (dc[d] == '/' ? 0 : n*n);
			h0 = gethead(p);
		}
		if (h1 < 0 && iscon(gx1, gy1, d))
		{
			int p = (gx1+dx[d])*n+(gy1+dy[d]);
			p += (dc[d] == '/' ? 0 : n*n);
			h1 = gethead(p);
		}
	}
	if (h0 > 0 && h1 > 0 && (h0 == h1))
		return true;
	return false;
}

//head[id] = -1
bool setline(int id)
{
	if (!isnul(id))
		return false;
	int h0 = -1, h1 = -1;
	if (isUnit(id, h0, h1))
		return false;
	if (h0 < 0 && h1 < 0)
		head[id] = id;
	else if (h0 >= 0 && h1 < 0)
		head[id] = h0;
	else if (h0 < 0 && h1 >= 0)
		head[id] = h1;
	else
	{
		head[id] = h1;
		head[h0] = h1;
	}
	return true;
}

bool setline(int x, int y, int d, bool con)
{
	int nx = x+dx[d], ny = y+dy[d];
	if (nx >= 0 && nx < n && ny >= 0 && ny < n)
	{
		int id = nx*n+ny + (('\\' == (con ? dc[d] : ndc[d])) ? n*n : 0);
		return setline(id);
	}
	return false;
}

void prepare()
{
	for(int i = 0; i < n+1; i++)
		for(int j = 0; j < n+1; j++)
		{
			if ('0' == maze[i][j])
			{
				for(int d=0; d < 4; d++)
					if (isnul(i, j, d))
						setline(i, j, d, false);
			}
		}
	bool running = true;
	while(running)
	{
		running = false;
		for(int i = 0; i < n+1; i++)
			for(int j = 0; j < n+1; j++)
			{
				if (maze[i][j] > '0' && maze[i][j] <= '9')
				{
					int nu = 0, c = 0;
					for(int d = 0; d < 4; d++)
					{
						nu += isnul(i, j, d) ? 1 : 0;
						c += iscon(i, j, d) ? 1 : 0;
					}
					if (nu > 0 && nu + c == maze[i][j]-'0')
					{
						running = true;
						for(int d = 0; d < 4; d++)
							if (isnul(i, j, d))
								setline(i, j, d, true);
					}
				}
			}
	}
}

