		p[strlen(p)-1] = 0;
		n = strlen(p)-1;
		memset(a, 0, sizeof(a));
		solve = false;
		dfs(0, true, Rec(0, '+', 0, false, 0));
		if (!solve)
			cout<<"  "<<"IMPOSSIBLE"<<endl;
	}
}
