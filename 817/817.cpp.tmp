const char *ins = "_+-*";
void dfs(int d, bool c, Rec rec)
{
	if (d == n)
		printans(rec);
	else
	{
		for(int i = (c ? 0 : 1); i < 4; i++)
		{
			Rec nrec = rec;
			a[d] = ins[i];
			
			if (0 == i)
			{
				if (nrec.m)
					nrec.c = nrec.c*10+p[d+1]-'0';
				else
					nrec.b = nrec.b*10+p[d+1]-'0';
			}
			else if(1 == i || 2 == i)
			{
				nrec.calc();
				nrec.b = p[d+1]-'0';
				nrec.e = ins[i]; 
			}
			else
			{
				if (nrec.m)
					nrec.b *= nrec.c;
				else
					nrec.m = true;
				nrec.c = p[d+1]-'0';
			}
			dfs(d+1, (i != 0 && p[d+1] == '0') ? false : true, nrec);
		}
	}
}

int main()
{
	int ka=0;
	while(cin>>p && '=' != p[0])
