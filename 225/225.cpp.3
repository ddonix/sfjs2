#include <iostream>
#include <string>
#include <cstring>
#include <queue>
using namespace std;

const int maxe = 30;
const int maxb = maxe*(maxe+2);
const int hmaxb = maxe*(maxe+2)/2;

int ed;
int bl;

struct Point
{
	int x;
	int y;
	Point() {}
	Point(int x_, int y_) : x(x_), y(y_) {}
	bool operator<(const Point & b) const
	{
		return (x < b.x) || (x == b.x && y < b.y);
	}
	
	bool operator==(const Point & b) const
	{
		return (x == b.x) && (y == b.y);
	}
};

queue<string> ans;
Point be(hmaxb,hmaxb);

//p :当前位置
//st:当前走几步
//e :当前走了多少步
//s :之前路径

string dir = "ensw";
int dx[] = {1, 0,  0, -1};
int dy[] = {0, 1, -1,  0};

bool vis[maxe*(maxe+2)][maxe*(maxe+2)];

bool visit(Point p, int e, int d)
{
	int x = p.x, y=p.y;
	for(int i = 1; i <= e; i++)
	{
		if (x+i*dx[d] == be.x && y+i*dy[d] == be.y && i == e && e == ed)
			return true;
		if (vis[x+i*dx[d]][y+i*dy[d]])
			return false;
	}
	for(int i = 1; i <= e; i++)
		vis[x+i*dx[d]][y+i*dy[d]] = true;
	return true;
}

void unvisit(Point p, int e, int d)
{
	int x = p.x, y=p.y;
	for(int i = 1; i <= e; i++)
		vis[x+i*dx[d]][y+i*dy[d]] = false;
}

void dfs(Point p, int e, string trace)
{
	if (p == be && e == ed)
	{
		ans.push(trace);
		return;
	}
	else
	{
		if (e == ed)
			return;
		bool df[4] = {true, true, true, true};
		if (!trace.empty())
		{
			if (trace.back() == 'n' || trace.back() == 's')
				df[1] = df[2] = false;
			else
				df[0] = df[3] = false;
		}
		for(int i = 0; i < 4; i++)
		{
			if (df[i] && visit(p, e+1, i))	//访问
			{
				Point np = Point(p.x + dx[i]*(e+1), p.y+dy[i]*(e+1));
				dfs(np, e+1, trace+dir[i]);
				unvisit(p, e+1, i);
			}
		}
	}
}

int main()
{
	int T;
	cin>>T;
	while(T--)
	{
		cin>>ed>>bl;
		memset(vis, 0, sizeof(vis));
		for(int i = 0; i < bl; i++)
		{
			int x,y;
			cin>>x>>y;
			vis[be.x + x][be.y + y] = true;
		}

		string s = "";
		while(!ans.empty()) ans.pop();
	
		vis[be.x][be.y] = true;
		dfs(be, 0, s);
		int t = ans.size();
		while(!ans.empty())
		{
			cout<<ans.front()<<endl;
			ans.pop();
		}
		cout<<"Found "<<t<<" golygon(s)."<<endl;
		cout<<ed<<" * "<<bl<<endl;
		if(T)
			cout<<endl;
	}
}
